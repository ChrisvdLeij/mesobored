ilter.name === target) {\n            filter.checked = true;\n            console.log(filter.query);\n          }\n        }\n      } else {\n        for (var filter of this.filters) {\n          if (filter.name === target) {\n            filter.checked = false;\n          }\n        }\n      }\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\n\nul {\n  list-style-type: none;\n  padding: 0;\n}\n\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\n\na {\n  color: #42b983;\n}\n</style>\n"]}]}